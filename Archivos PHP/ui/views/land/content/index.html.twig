{% extends "land/structure/base.html.twig" %}{% block contenido %}    <!-- Page Content-->    <!-- Main Slider-->    <section class="hero-slider" style="background-image: url({{ base }}ui/assets/templates/commer/img/hero-slider/main-bg.jpg);">        <div class="owl-carousel large-controls dots-inside"             data-owl-carousel="{ &quot;nav&quot;: true, &quot;dots&quot;: true, &quot;loop&quot;: true, &quot;autoplay&quot;: true, &quot;autoplayTimeout&quot;: 7000 }">            {% for data in hd_sliders %}            <div class="item">                <div class="container padding-top-3x">                    <div class="row justify-content-center align-items-center">                        <div class="col-lg-5 col-md-6 padding-bottom-2x text-md-left text-center">                            <div class="from-bottom">                               <!-- <img class="d-inline-block mb-4" src="{{ base }}ui/assets/templates/commer/img/hero-slider/logo03.png" style="width: 125px;" alt="Motorola"> -->                                <div class="h2 text-body text-normal mb-2 pt-1">{{ data.title }}</div>                                <p>{{ data.description }}</p>                            </div>                            <a class="btn btn-primary scale-up delay-1" href="{{ data.link }}">Ver mas</a>                        </div>                        <div class="col-md-6 padding-bottom-2x mb-3">                            <img class="d-block mx-auto" src="{{ base }}{{ data.image }}" alt="{{ data.title }}">                        </div>                    </div>                </div>            </div>            {% endfor %}        </div>    </section>    <!-- Top Categories-->    <section class="container padding-top-3x">        <h3 class="text-center mb-30">Top productos</h3>        <div class="row">            {% for product in product_top %}                <div class="col-md-4 col-sm-6">                    <div class="card mb-30">                        <a class="card-img-tiles" href="{{ base }}product/view/{{ product.product.id_product }}">                            <div class="inner">                                <div class="main-img">                                    <img src="{{ base }}{{ product.images[0].rute }}" alt="Category">                                </div>                                {% if product.images[1].rute != "" %}                                    <div class="thumblist">                                        {% if product.images[1].rute != "" %}                                            <img src="{{ base }}{{ product.images[1].rute }}" alt="Category">                                        {% endif %}                                        {% if product.images[2].rute != "" %}                                            <img src="{{ base }}{{ product.images[2].rute }}" alt="Category">                                        {% endif %}                                    </div>                                {% endif %}                            </div>                        </a>                        <div class="card-body text-center">                            <h4 class="card-title">{{ product.product.name }}</h4>                            <p class="text-muted">                                {% if product.product.porcent_discount != 0 %}                            <h4 class="text-success">                                ${{ ( product.product.price - (product.product.price * product.product.porcent_discount / 100 ))|number_format(2, '.', ',') }}                                <del>                                    <small> | ${{ product.product.price|number_format(2, '.', ',') }}</small>                                </del>                            </h4>                            <h5>                                <small> %{{ product.product.porcent_discount }} de descuento</small>                            </h5>                            {% else %}                                <h5> ${{ product.product.price|number_format(2, '.', ',') }}</h5>                                <br>                            {% endif %}                            </p>                            <div class="row">                                <div class="col-md-6 col-xs-12">                                    <a href="{{ base }}product/view/{{ product.product.id_product }}"                                       class="btn btn-outline-primary btn-sm">Ver producto</a>                                </div>                                <div class="col-md-6 col-xs-12">                                    <form class="formNormal" id="{{ base }}shopping/add" action="#" method="post">                                        <input type="hidden" name="product[id_product]"                                               value="{{ product.product.id_product }}">                                        <input type="hidden" name="product[q]" value="1">                                        <button type="submit" class="btn btn-outline-primary btn-sm">Comprar</button>                                    </form>                                </div>                            </div>                        </div>                    </div>                </div>            {% endfor %}        </div>    </section>    <section class="bg-faded padding-top-3x padding-bottom-3x">        <div class="container">            <h3 class="text-center mb-30 pb-2">Nuestras marcas</h3>            <div class="owl-carousel"                 data-owl-carousel="{ &quot;nav&quot;: false, &quot;dots&quot;: false, &quot;loop&quot;: true, &quot;autoplay&quot;: true, &quot;autoplayTimeout&quot;: 4000, &quot;responsive&quot;: {&quot;0&quot;:{&quot;items&quot;:2}, &quot;470&quot;:{&quot;items&quot;:3},&quot;630&quot;:{&quot;items&quot;:4},&quot;991&quot;:{&quot;items&quot;:5},&quot;1200&quot;:{&quot;items&quot;:6}} }">                {% for mark in hd_mark %}                    <img class="d-block w-110 opacity-75 m-auto" src="{{ base }}{{ mark.image }}"                         alt="{{ base }}{{ mark.name }}">                {% endfor %}            </div>        </div>    </section>    <!-- Services-->    <section class="container padding-top-3x padding-bottom-2x">        <div class="row">            <div class="col-md-3 col-sm-6 text-center mb-30"><img                        class="d-block w-90 img-thumbnail rounded-circle mx-auto mb-3"                        src="{{ base }}ui/assets/templates/commer/img/services/01.png"                        alt="Shipping">                <h6>Envío gratis a todo el mundo</h6>                <p class="text-muted margin-bottom-none">Envío gratis para todos los pedidos superiores a $ 1000</p>            </div>            <div class="col-md-3 col-sm-6 text-center mb-30"><img                        class="d-block w-90 img-thumbnail rounded-circle mx-auto mb-3"                        src="{{ base }}ui/assets/templates/commer/img/services/02.png"                        alt="Money Back">                <h6>Garantía de devolución del dinero</h6>                <p class="text-muted margin-bottom-none">Devolvemos dinero en 30 días</p>            </div>            <div class="col-md-3 col-sm-6 text-center mb-30"><img                        class="d-block w-90 img-thumbnail rounded-circle mx-auto mb-3"                        src="{{ base }}ui/assets/templates/commer/img/services/03.png"                        alt="Support">                <h6>Atención al cliente 24/7</h6>                <p class="text-muted margin-bottom-none">Atención al cliente amigable 24/7</p>            </div>            <div class="col-md-3 col-sm-6 text-center mb-30"><img                        class="d-block w-90 img-thumbnail rounded-circle mx-auto mb-3"                        src="{{ base }}ui/assets/templates/commer/img/services/04.png"                        alt="Payment">                <h6>Pago seguro en línea</h6>                <p class="text-muted margin-bottom-none">Pagos por metodos seguros / Paypal</p>            </div>        </div>    </section>    <!-- Site Footer-->{% endblock %}